<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="carbon.MainWindow">
    <TabPane tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="introTab" text="Intro">
            <fx:include source="intro.fxml"/>
        </Tab>
        <Tab fx:id="activitiesTab" text="Activities">
            <fx:include fx:id="carbon" source="carbon.fxml"/>
        </Tab>
        <Tab text="Results">
            <fx:include fx:id="resultsTab" source="resultsTab.fxml"/>
        </Tab>
    </TabPane>
</AnchorPane>
