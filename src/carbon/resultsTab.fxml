<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="carbon.ResultsTab"
          prefHeight="400.0" prefWidth="600.0">
    <HBox alignment="CENTER_LEFT" fx:id="header">
        <Text>Order By: </Text>
        <Button onMouseClicked="#orderActivitiesByDate" fx:id="orderByDate" text="Date"/>
        <Button onMouseClicked="#orderActivitiesByName" fx:id="orderByName" text="Name"/>
    </HBox>
    <GridPane fx:id="activitiesGrid">
        <Button></Button>
    </GridPane>
</VBox>
